             xmlns:local="using:GradeManagement"
             x:Class="GradeManagement.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
    
    <Application.DataContext>
        <local:App/>
    </Application.DataContext>

    <Application.Styles>
        <FluentTheme Mode="Light"/>
        <StyleInclude Source="/src/Resources/BaseResources.axaml"/>
    </Application.Styles>
</Application>
